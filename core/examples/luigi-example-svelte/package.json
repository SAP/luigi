{
  "name": "luigi-example-svelte",
  "version": "0.1.0",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.13.1",
    "serve": "^11.0.0",
    "style-loader": "^0.23.1",
    "svelte": "^3.20.1",
    "svelte-loader": "2.13.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "scripts": {
    "build:prod": "NODE_ENV=production webpack --display-error-details",
    "build:dev": "webpack",
    "dev": "concurrently \"npm run build:dev \" \"npm run serve:luigi\" \"npm run serve:microfrontend\"",
    "serve": "concurrently \"npm run build:prod \" \"npm run serve:luigi\" \"npm run serve:microfrontend\"",
    "serve:luigi": "sirv public --single",
    "serve:microfrontend": "cd public && ws --spa mf.html --port 5001"
  },
  "dependencies": {
    "@luigi-project/client": "1.0.1",
    "@luigi-project/core": "1.0.1",
    "concurrently": "^5.1.0",
    "fundamental-styles": "^0.8.1",
    "@sap-theming/theming-base-content": "^11.1.18",
    "local-web-server": "^3.0.7",
    "svelte-routing": "^1.4.2",
    "sirv-cli": "^0.4.5"
  }
}
