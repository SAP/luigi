{
  "name": "luigi-example-svelte",
  "version": "0.1.0",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "mini-css-extract-plugin": "^0.6.0",
    "serve": "^11.0.0",
    "style-loader": "^0.23.1",
    "svelte": "^3.0.0",
    "svelte-loader": "2.13.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "scripts": {
    "buildSvelte": "webpack --config webpack.config",
    "dev": "concurrently \"npm run buildSvelte -- --watch\" \"npm run serve:dev\" \"npm run serve:luigi\"",
    "serve:dev": "sirv public --single --dev --port 5000",
    "serve:luigi": "cd public && ws --spa mf.html --port 5001"
  },
  "dependencies": {
    "@kyma-project/luigi-client": "0.7.3",
    "@kyma-project/luigi-core": "0.7.3",
    "concurrently": "^5.1.0",
    "fiori-fundamentals": "1.6.0",
    "local-web-server": "^3.0.7",
    "sirv-cli": "^0.4.5",
    "svelte-routing": "^1.4.0"
  }
}
