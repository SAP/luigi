{
  "name": "LuigiOpenUI5",
  "private": true,
  "version": "1.0.0",
  "author": "SAP SE",
  "description": "Luigi with OpenUI5 Demo App",
  "scripts": {
    "build": "ui5 build preload --all=true",
    "start": "ui5 serve --open index.html",
    "serve": "ws --compress -d dist",
    "postinstall": "cp -r node_modules/@kyma-project/luigi-core webapp/libs/luigi-core && cp -r node_modules/@kyma-project/luigi-client webapp/libs/luigi-client"
  },
  "dependencies": {
    "@kyma-project/luigi-client": "^0.7.3",
    "@kyma-project/luigi-core": "^0.7.3",
    "@openui5/sap.f": "^1.73.2",
    "@openui5/sap.m": "^1",
    "@openui5/sap.ui.core": "^1",
    "@openui5/themelib_sap_fiori_3": "^1.73.2",
    "local-web-server": "^3.0.7"
  }
}
