<mvc:View
	controllerName="luigi.demo.controller.Home"
	xmlns="sap.m"
	xmlns:f="sap.f"
	xmlns:card="sap.f.cards"
	xmlns:mvc="sap.ui.core.mvc">
	<VBox class="sapUiMediumMargin">
		<Title  class="sapUiSmallMargin" titleStyle="H1" text="{i18n>homeTitle}" wrapping="true" />
		<f:Card class="sapUiSmallMargin">
			<f:content>
				<Title  class="sapUiSmallMargin" titleStyle="H4" id="luigi-initialized"  text="" wrapping="true" />
			</f:content>
		</f:Card>
	</VBox>
</mvc:View>