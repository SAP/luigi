{
   "js": {
      "parser": "babel-flow",
      "trailingComma": "none",
      "tabWidth": 2,
      "singleQuote": true,
      "printWidth": 120
   },
   "ts": {
      "parser": "typescript",
      "trailingComma": "none",
      "tabWidth": 2,
      "singleQuote": true,
      "printWidth": 120
   },
   "html": {
      "parser": "html",
      "tabWidth": 2,
      "singleQuote": true,
      "printWidth": 90
   },
   "svelte": {
      "tabWidth": 2,
      "singleQuote": true,
      "printWidth": 90
   }
}
