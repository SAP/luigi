{
   "js": {
      "parser": "babel-flow",
      "trailingComma": "none",
      "tabWidth": 2,
      "singleQuote": true,
      "printWidth": 120
   },
   "ts_disabled": {
      "parser": "typescript",
      "trailingComma": "none",
      "tabWidth": 2,
      "singleQuote": true,
      "printWidth": 120
   },
   "html": {
      "parser": "html",
      "tabWidth": 2,
      "singleQuote": true,
      "printWidth": 90
   },
   "svelte": {
      "parser": "svelte",
      "plugins": ["prettier-plugin-svelte"],
      "pluginSearchDirs": ["."],
      "svelteAllowShorthand": true,
      "svelteBracketNewLine": false,
      "svelteIndentScriptAndStyle": true,
      "svelteSortOrder": "options-scripts-markup-styles",
      "svelteStrictMode": false
   }
}
