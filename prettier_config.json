{
   "js": {
      "parser": "babel-flow",
      "trailingComma": "es5",
      "tabWidth": 2,
      "singleQuote": true,
      "printWidth": 120
   },
   "ts": {
      "parser": "typescript",
      "trailingComma": "es5",
      "tabWidth": 2,
      "singleQuote": true,
      "printWidth": 120
   },
   "html": {
      "parser": "html",
      "tabWidth": 2
   }
}
