{
  "name": "luigi-core-material-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@luigi-project/core": "^1.3.0",
    "@luigi-project/client": "^1.4.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3"
  },
  "scripts": {
    "start": "concurrently \"npm run config -- --watch\" \"npm run start:react\"",
    "start:react": "PORT=3003 react-scripts start",
    "config": "webpack --config webpack-generateConfig.config",
    "build": "npm run config && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "react-app"
    ],
    "globals": {
      "Luigi": "readonly"
    },
    "rules": {
      "no-undef": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.1.0",
    "node-sass": "^4.14.1",
    "webpack-cli": "^3.3.12",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1"
  }
}
